                                    "align": "center",
                                    "allowSorting": true,
                                    "isContentHtml": true,
                                    "width": ".75*"
                                },
                                // {
                                //     "header": "Edit",
                                //     "binding": "editId",
                                //     "width": ".25*",
                                //     "align": "center",
                                //     "isContentHtml": true

                                // },
                                // {
                                //     "header": "Delete",
                                //     "binding": "deleteId",
                                //     "width": ".25*",
                                //     "align": "center",
                                //     "isContentHtml": true

