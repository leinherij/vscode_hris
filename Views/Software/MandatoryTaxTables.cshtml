                            });
                        }
                        taxCodeCollection = new wijmo.collections.CollectionView(taxCodeList);
                        btnCurrentTaxCodeGrid.innerHTML = (taxCodeCollection.pageIndex + 1) + ' / ' + taxCodeCollection.pageCount;
                        taxCodeCollection.canFilter = true;
                        taxCodeCollection.pageSize = 15;
                        if(taxCodeGrid){
                            taxCodeGrid.dispose();
                        }   
                        //Flex Grid
                        taxCodeGrid = new wijmo.grid.FlexGrid('#taxCodeGrid');
 
                            
                        
                        taxCodeGrid.initialize({
                            
                            columns: [
                                {
                                    "header": "Tax Code",
                                    "binding": "taxCode",
