    function updateNavigateSSSCollection() {
        if (sssCollection.pageSize <= 0) {
            document.getElementById('navigationSSSGrid').style.display = 'none';
            return;
        }
        document.getElementById('navigationSSSGrid').style.display = 'block';
        if (sssCollection.pageIndex === 0) {
            btnFirstSSSGrid.setAttribute('disabled', 'disabled');
            btnPreviousSSSGrid.setAttribute('disabled', 'disabled');
            btnNextSSSGrid.removeAttribute('disabled');
            btnLastSSSGrid.removeAttribute('disabled');
        } else if (sssCollection.pageIndex === (sssCollection.pageCount - 1)) {
            btnFirstSSSGrid.removeAttribute('disabled');
